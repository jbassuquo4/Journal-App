<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.jerry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Journal App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".journalapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".journalapp.MenuScreen" />
        <activity android:name=".journalapp.UpdateActivity" />
        <activity android:name=".journalapp.ViewActivity" />
        <activity android:name=".journalapp.LoginActivity"></activity>
    </application>

</manifest>